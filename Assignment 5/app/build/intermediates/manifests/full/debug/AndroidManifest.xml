<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.example.sudoku"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="3"
        android:targetSdkVersion="10" />

    <application
        android:debuggable="true"
        android:icon="@drawable/prehistoric_florida_logo3_512"
        android:label="@string/app_name" >
        <activity
            android:name="org.example.sudoku.MODSApp"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.example.sudoku.About"
            android:label="@string/about_title"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="org.example.sudoku.Prefs"
            android:label="@string/settings_title" >
        </activity>
        <activity
            android:name="org.example.sudoku.PrehistoricAnimals"
            android:label="Florida&apos;s Prehistoric Animals" >
        </activity>
        <activity
            android:name="org.example.sudoku.TriviaGame"
            android:label="A Trivia Game" >
        </activity>
        <activity
            android:name="org.example.sudoku.SeaLevel"
            android:label="Sealevel Changes" >
        </activity>
        <activity
            android:name="org.example.sudoku.Fossils"
            android:label="Paleontology and Fossils" >
        </activity>
        <activity
            android:name="org.example.sudoku.animals.p_megatherium"
            android:label="Megatherium" >
        </activity>
        <activity
            android:name="org.example.sudoku.animals.p_woolymammoth"
            android:label="Woolly Mammoth" >
        </activity>
        <activity
            android:name="org.example.sudoku.animals.p_sabertoothedcat"
            android:label="Sabertooth Tiger" >
        </activity>
        <activity
            android:name="org.example.sudoku.animals.p_phorusrhacidae"
            android:label="Phorushacidae" >
        </activity>
        <activity
            android:name="org.example.sudoku.FossilsMoreInfo"
            android:label="More info about Paleontology and Fossils" >
        </activity>
    </application>

</manifest>